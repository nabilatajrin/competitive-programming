•    #include <stdio.h>
•    int main()
•    {
•        int i,j,n;
•        while(scanf("%d",&n)!=EOF)
•        {
•          for(i=1;i<=n;i++)
•          {
•              for(j=1;j<=n;j++)
•              {
•                  printf("*");
•              }
•              printf("\n");
•          }
•        }
•        return 0;
•    }
