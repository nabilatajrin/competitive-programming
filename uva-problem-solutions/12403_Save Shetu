#include <stdio.h>
#include <string.h>

int main()
{
    int i,x,n,output;
    char ch[7],donate,report;

    output=0;
    scanf("%d",&x);
    for(i=0;i<x;i++)
    {
        scanf("%s",ch);
        if (strcmp(ch,"donate")==0)
        {
            scanf("%d",&n);
            output=output+n;
        }
        if (strcmp(ch,"report")==0)
        {
            printf("%d\n",output);
        }
    }
    return 0;
}
