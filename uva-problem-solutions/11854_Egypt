#include <stdio.h>

int main()
{
    int x,y,z;

    while(scanf("%d%d%d",&x,&y,&z)!=0)
    {
        if(x==0&&y==0&&z==0)
        {
            break;
        }
        else
        {
            if((x*x+y*y==z*z)||(y*y+z*z==x*x)||(z*z+x*x==y*y))
            {
                printf("right\n");
            }
            else
            {
                printf("wrong\n");
            }
        }
    }
    return 0;
}
