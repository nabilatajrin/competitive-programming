#include <stdio.h>

int main()
{
    long long x,y,sub;

    while(scanf("%lld%lld",&x,&y)!=EOF)
    {
        if(x>=y)
        {
            sub=x-y;
            printf("%lld\n",sub);
        }

        else
        {
            sub=y-x;
            printf("%lld\n",sub);
        }
    }
    return 0;
}
